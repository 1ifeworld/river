{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2016",
    "module": "ESNext",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": false,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  }
}
